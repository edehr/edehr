{
  "name": "ehr-workspace",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "nonscripts": {
    "start:nm": "npx babel-node src/index.js",
    "build": "npm run clean && npm run build-babel && npm run build-resources",
    "build-babel": "babel -d ./build ./src/index.js -s",
    "start:dev": "nodemon --exec babel-node src/index.js"
  },
  "scripts": {
    "build:resources": "mkdir -p build/resources && cp src/resources/* build/resources",
    "bb": "npm run clean && npm run build:resources && npx babel src --out-dir build",
    "clean": "rm -rf build && mkdir build",
    "lint": "eslint --fix 'src/ehr-definitions/**/*.js'",
    "start": "npm run bb && npm run build-resources && npm run start:run",
    "start:run": "node build/index.js",
    "test": "npm run bb && node_modules/.bin/mocha --recursive --sort --bail 'build/**/*.spec.js'",
    "test:all": "npm run bb && node_modules/.bin/mocha --recursive --sort 'build/**/*.spec.js'",
    "test:cov": "npm run bb && nyc -x '**/*.spec.js' node_modules/.bin/mocha --recursive --sort --bail  'build/**/*.spec.js'",
    "test:mar": "npm run bb && node_modules/.bin/mocha 'build/ehr-tests/mar-timeline.spec.js'",
    "test:one": "npm run bb && node_modules/.bin/mocha 'build/ehr-tests/ehr-data-model.spec.js'"
  },
  "dependencies": {
    "nyc": "^15.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.16.0",
    "@babel/runtime": "^7.20.7",
    "camelcase": "^6.2.1",
    "chai": "^4.3.4",
    "eslint": "^8.4.1",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.20",
    "should": "^13.2.3"
  }
}
