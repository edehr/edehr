{
  "name": "edehr-server",
  "title": "Ed EHR",
  "keywords": [
    "BCCampus",
    "Learning Tools Interoperability",
    "electronic Health Record System"
  ],
  "description": "This project provides an educational tool that replicates an Electronic Health Record system within a Learning Tools Interoperability (LTI) compliant course-ware.",
  "scripts": {
    "start": "NODE_ENV=development DEBUG='server, error' node index.js",
    "start:dev": "NODE_ENV=development DEBUG='server, error' nodemon -L  node index.js",
    "lint": "eslint --fix src/**/*.js",
    "test": "node_modules/.bin/mocha --recursive --sort --bail --require @babel/register --require @babel/runtime/regenerator **/*.spec.js",
    "test-one": "NODE_ENV=development DEBUG='server, error' node_modules/.bin/mocha --require @babel/register src/mcr/files/files-controller.spec.js",
    "test:cov": "nyc -x '**/*.spec.js' node_modules/.bin/mocha --recursive --sort --bail --require @babel/register **/*.spec.js",
    "test:server": "node_modules/.bin/mocha --recursive --sort --bail --require @babel/register **/*.server.spec.js",
    "test:system": "node_modules/.bin/mocha --recursive --sort --bail --require @babel/register **/*.system.spec.js",
    "start-docker": "node index.js",
    "start-docker-dev": "nodemon -L  node index.js",
    "start-prod": "NODE_ENV=production node index.js",
    "scale-test": "node test/scale-testing.js",
    "generate-password": "node src/server/generate-password.js"
  },
  "comments": {
    "mongoose": "Had to pin the version at 5.9.29. See https://github.com/vercel/next.js/issues/16290 Will need to manually upgrade when mongoose is fixed."
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "deepmerge": "^2.2.1",
    "ejs": "^2.6.1",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "express-session": "^1.17.1",
    "filenamify": "^4.1.0",
    "filesize": "^6.1.0",
    "helmet": "^3.23.3",
    "ims-lti": "git+https://github.com/bryan-gilbert/ims-lti.git",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.26.0",
    "mongoose": "5.9.29",
    "mongoose-unique-validator": "^2.0.3",
    "multer": "^1.4.2",
    "nanoid": "^3.1.12",
    "passport": "^0.4.0",
    "passport-custom": "^1.1.1",
    "pluralize": "^7.0.0",
    "pug": "^3.0.1",
    "qs": "^6.9.4",
    "session-file-store": "^1.2.0",
    "sinon": "^9.0.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "camelcase": "^5.3.1",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "coveralls": "^3.1.0",
    "eslint": "^5.16.0",
    "mocha": "^5.2.0",
    "nodemon": "^2.0.4",
    "nyc": "^14.1.1",
    "should": "^13.2.3",
    "supertest": "^4.0.2"
  }
}
