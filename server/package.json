{
  "name": "edehr-server",
  "title": "Ed EHR",
  "version": "1.1.1",
  "version-comment": "Need to manually update version when git version tag is changed.",
  "keywords": [
    "BCCampus",
    "Learning Tools Interoperability",
    "electronic Health Record System"
  ],
  "description": "This project provides an educational tool that replicates an Electronic Health Record system within a Learning Tools Interoperability (LTI) compliant course-ware.",
  "scripts": {
    "clean": "rm -rf build",
    "build:babel": "mkdir -p build && babel src --out-dir ./build",
    "build": "npm run clean && npm run build:babel",
    "start": "NODE_ENV=development DEBUG='server, error' nodemon --exec babel-node src/index.js",
    "start:prod": "NODE_ENV=production DEBUG='error' node build/index.js",
    "lint": "eslint --fix src/**/*.js",
    "test": "npm run build && node_modules/.bin/mocha --recursive --sort --bail build/**/*.*.spec.js",
    "test:verbose": "NODE_ENV=development DEBUG='server, error' && npm run test",
    "test:one": "NODE_ENV=development DEBUG='server, error' node_modules/.bin/mocha --require @babel/register --require @babel/runtime/regenerator --require @babel/polyfill src/mcr/activity-data/activityData.server.spec.js",
    "test:cov": "npm run build && nyc -x '**/*.spec.js' node_modules/.bin/mocha --recursive --sort --bail  build/**/*.spec.js",
    "test:server": "npm run build && node_modules/.bin/mocha --recursive --sort --bail  build/**/*.server.spec.js",
    "test:system": "npm run build && node_modules/.bin/mocha --recursive --sort --bail build/**/*.system.spec.js",
    "scale-test": "node test/scale-testing.js",
    "generate-password": "node src/server/generate-password.js"
  },
  "comments": {},
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.4",
    "debug": "~4.3.3",
    "deepmerge": "^4.2.2",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.1",
    "express-session": "^1.17.2",
    "filenamify": "^5.0.2",
    "filesize": "^8.0.6",
    "helmet": "^4.6.0",
    "ims-lti": "git+https://github.com/bryan-gilbert/ims-lti.git",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.13.14",
    "mongoose-unique-validator": "^2.0.3",
    "multer": "^1.4.4",
    "nanoid": "^3.1.31",
    "passport": "^0.5.0",
    "passport-custom": "^1.1.1",
    "pluralize": "^8.0.0",
    "pug": "^3.0.2",
    "qs": "^6.10.2",
    "session-file-store": "^1.5.0",
    "sinon": "^12.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.4",
    "@babel/register": "^7.16.0",
    "camelcase": "^6.2.1",
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "eslint": "^8.4.1",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "should": "^13.2.3",
    "supertest": "^6.1.6"
  }
}
