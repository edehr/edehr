{
  "name": "edehr-client",
  "description": "This is the client sub-package of the parent EdEhr project",
  "scripts": {
    "setup": "cp sample.env .env",
    "start": "NODE_ENV=development VUE_APP_ROOT_API=http://localhost:27000/api vue-cli-service serve",
    "build": "vue-cli-service build",
    "publish": "cp -R dist/* published/",
    "both": "npm run build && npm run publish",
    "lint": "vue-cli-service lint",
    "test": "npm run test:unit",
    "test:cov": "vue-cli-service test:unit --coverage",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "test:updateSnapshots": "vue-cli-service test:unit -- -u",
    "test:it": "npm run test:unit -t src/inside/components/chart/charting.spec.js",
    "test:one": "npm run test:unit --trace-warnings -t src/helpers/test/page-defs.spec.js",
    "test:one:pg": "npm run test:unit --verbose --trace-warnings -t src/helpers/test/page-controller.spec.js",
    "test:one:cov": "npm run test:cov --trace-warnings -t src/helpers/test/page-defs.spec.js | egrep 'ehr-defs|validations'",
    "test:calcs": "npm run test:unit --trace-warnings -t src/helpers/test/ehr-calcs.spec.js",
    "test:calcs:cov": "npm run test:cov --trace-warnings -t src/helpers/test/ehr-calcs.spec.js | egrep 'ehr-calcs'",
    "test:defgrid": "npm run test:unit --trace-warnings -t src/helpers/test/ehr-defs-grid.spec.js"
  },
  "dependencies": {
    "@bryan-gilbert/typeahead-standalone": "^4.14.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^0.1.10",
    "@sentry/tracing": "^7.13.0",
    "@sentry/vue": "^7.13.0",
    "axios": "^0.21.4",
    "bulma": "^0.9.3",
    "core-js": "^3.8.3",
    "exif-js": "^2.3.0",
    "file-saver": "^2.0.5",
    "filesize": "^8.0.7",
    "moment": "^2.29.4",
    "node-sass": "^7.0.3",
    "qs": "^6.9.4",
    "sass": "^1.43.5",
    "valid-filename": "^3.1.0",
    "vue": "^2.6.14",
    "vue-autosuggest": "^2.2.0",
    "vue-moment": "^4.1.0",
    "vue-router": "^3.5.3",
    "vue-spinner-component": "^1.0.5",
    "vue-upload-component": "^2.8.22",
    "vuejs-datepicker": "^1.5.4",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "^5.0.1",
    "@vue/cli-plugin-e2e-nightwatch": "^5.0.1",
    "@vue/cli-plugin-eslint": "^5.0.1",
    "@vue/cli-plugin-unit-jest": "^5.0.1",
    "@vue/cli-service": "^5.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "@vue/vue2-jest": "^27.0.0-alpha.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^25.4.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.1.0",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "sass-loader": "^9.0.3",
    "should": "^13.2.3",
    "vue-jest": "^4.0.1",
    "vue-template-compiler": "^2.6.14"
  }
}
